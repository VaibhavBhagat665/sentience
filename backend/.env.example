# Sentience Node Environment Configuration

# Server Configuration
PORT=3000
NODE_ENV=development

# Aptos Configuration
APTOS_NETWORK=testnet
APTOS_NODE_URL=https://fullnode.testnet.aptoslabs.com/v1
APTOS_FAUCET_URL=https://faucet.testnet.aptoslabs.com

# Server Wallet (x402 Payment Recipient)
SERVER_WALLET=0x_YOUR_SERVER_WALLET_ADDRESS
SERVER_PRIVATE_KEY=0x_YOUR_PRIVATE_KEY

# x402 Payment Configuration
X402_MIN_AMOUNT=500000
X402_ALLOW_UNCONFIRMED=true
X402_RETRY_INTERVAL=1000

# Contract Addresses (update after deployment)
SENTIENCE_MODULE_ADDRESS=0x_YOUR_MODULE_ADDRESS

# Reputation Thresholds
MIN_TRUST_SCORE=10000000
REPUTATION_ORACLE_ADDRESS=0x_ORACLE_ADDRESS

# Easter Egg Configuration
MAGIC_SPELL=0xf2dbdeb981aca16eb5cb33eab7
